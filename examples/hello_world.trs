type char is 1 bytes;
type int is 8 bytes;

char[] greeting = "Hi there!\n";

enum int syscalls {write = 1, exit = 60};
enum int streams { in, out, err };

register rsi is char[0];
register rdx is int;
register rax is int;
register rdi is int;

entry {
	mov rsi, greeting.data;
	mov rdx, greeting.count;
	mov rax, syscalls.write;
	mov rdi streams.out;
	syscall;

	mov rax, syscalls.exit;
	mov rdi, 0;
	syscall;
}
